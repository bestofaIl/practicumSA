  â• â•â•¡ =#

# â•”â•â•¡ 6bd34541-e5b6-409e-b2ff-8f51ce57f74d
# â• â•â•¡ disabled = true
#=â• â•â•¡
begin
	tmp = [4,6,3];
	tmp2 = [];

	function count_6s_in_number(num)
    	count = 0
    	while num != 0
        	if num % 10 == 6
            	count += 1
        	end
        	num = div(num, 10)
    	end
    	return count
	end


	push!(tmp2, 2^tmp[1])
	push!(tmp2, 2^tmp[2])
	
	for i in 1:1:4
		push!(tmp2, 2^tmp[3])
	end
	
	println(tmp2)


	let amount_six = 0
	for i in 1:1:length(tmp2)
		amount_six += count_6s_in_number(tmp2[i])
	end

	println(amount_six)
	
end
end	
