begin
	A = Set([0, 3, 4, 9]);
	B = Set([1, 3, 4, 7]);
	C = Set([0, 1, 2, 4, 7, 8, 9]);
	# ğ‘ƒ = ğ´ âˆ© ğµ âˆª ğ´ âˆ© ğµ âˆª ğ´ âˆ© ğ¶ âˆª ğµ âˆ© ğ¶.
	P = union(intersect(A,B),intersect(A,B),intersect(A,C),intersect(B,C))
	println(P)
end
