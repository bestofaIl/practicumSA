	# Ğ¾Ğ´Ğ½Ğ¾Ğ¼ĞµÑ€Ğ½Ñ‹Ğ¹ Ğ¼Ğ°ÑÑĞ¸Ğ² Ğ¸Ğ· 4 Ğ½ÑƒĞ»ĞµĞ¹:
	zeros(4)
	# Ğ·Ğ°Ğ¿Ğ¾Ğ»Ğ½Ğ¸Ñ‚ÑŒ Ğ¼Ğ°ÑÑĞ¸Ğ² 3x2 Ñ†Ğ¸Ñ„Ñ€Ğ°Ğ¼Ğ¸ 3.5
	fill(3.5,(3,2))
	# Ğ·Ğ°Ğ¿Ğ¾Ğ»Ğ½ĞµĞ½Ğ¸Ğµ Ğ¼Ğ°ÑÑĞ¸Ğ²Ğ° Ğ¿Ğ¾ÑÑ€ĞµĞ´ÑÑ‚Ğ²Ğ¾Ğ¼ Ñ„ÑƒĞ½ĞºÑ†Ğ¸Ğ¸ repeat():
	repeat([1,2],3,3)
	repeat([1 2],2,2)
	# Ğ¿Ñ€ĞµĞ¾Ğ±Ñ€Ğ°Ğ·Ğ¾Ğ²Ğ°Ğ½Ğ¸Ğµ Ğ¾Ğ´Ğ½Ğ¾Ğ¼ĞµÑ€Ğ½Ğ¾Ğ³Ğ¾ Ğ¼Ğ°ÑÑĞ¸Ğ²Ğ° Ğ¸Ğ· Ñ†ĞµĞ»Ñ‹Ñ… Ñ‡Ğ¸ÑĞµĞ» Ğ¾Ñ‚ 1 Ğ´Ğ¾ 12
	# Ğ² Ğ´Ğ²ÑƒĞ¼ĞµÑ€Ğ½Ñ‹Ğ¹ Ğ¼Ğ°ÑÑĞ¸Ğ² 2x6
	a = collect(1:12)
	b = reshape(a,(2,6))
	# Ñ‚Ñ€Ğ°Ğ½ÑĞ¿Ğ¾Ğ½Ğ¸Ñ€Ğ¾Ğ²Ğ°Ğ½Ğ¸Ğµ
	b'
	# Ñ‚Ñ€Ğ°Ğ½ÑĞ¿Ğ¾Ğ½Ğ¸Ñ€Ğ¾Ğ²Ğ°Ğ½Ğ¸Ğµ
	Ñ = transpose(b)
	# Ğ¼Ğ°ÑÑĞ¸Ğ² 10x5 Ñ†ĞµĞ»Ñ‹Ñ… Ñ‡Ğ¸ÑĞµĞ» Ğ² Ğ´Ğ¸Ğ°Ğ¿Ğ°Ğ·Ğ¾Ğ½Ğµ [10, 20]:
	ar = rand(10:20, 10, 5)
	# Ğ²Ñ‹Ğ±Ğ¾Ñ€ Ğ²ÑĞµÑ… Ğ·Ğ½Ğ°Ñ‡ĞµĞ½Ğ¸Ğ¹ ÑÑ‚Ñ€Ğ¾ĞºĞ¸ Ğ² ÑÑ‚Ğ¾Ğ»Ğ±Ñ†Ğµ 2:
	# Ğ²Ñ‹Ğ±Ğ¾Ñ€ Ğ²ÑĞµÑ… Ğ·Ğ½Ğ°Ñ‡ĞµĞ½Ğ¸Ğ¹ Ğ² ÑÑ‚Ğ¾Ğ»Ğ±Ñ†Ğ°Ñ… 2 Ğ¸ 5:
	ar[:, [2, 5]]
	# Ğ²ÑĞµ Ğ·Ğ½Ğ°Ñ‡ĞµĞ½Ğ¸Ñ ÑÑ‚Ñ€Ğ¾Ğº Ğ² ÑÑ‚Ğ¾Ğ»Ğ±Ñ†Ğ°Ñ… 2, 3 Ğ¸ 4:
