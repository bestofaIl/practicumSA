@btime eigvals(A)
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° f4f1a1ca-b6fc-41bc-99b2-c45674cd0c6f
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
matrix_new = zeros(5,5)
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° 245d3482-225d-4cf2-8a97-5950b691d74b
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
begin
	
		@btime for i in 1:1:5
			matrix_new[i, i] = eigenvalues[i]
		end

		
	matrix_new
end
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° a61ca57b-7e2c-4fe2-85dc-595fb8ae0d25
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
begin
	# –°–æ–∑–¥–∞–π—Ç–µ –Ω–∏–∂–Ω–µ–¥–∏–∞–≥–æ–Ω–∞–ª—å–Ω—É—é –º–∞—Ç—Ä–∏—Ü—É –∏–∑ –º–∞—Ç—Ä–∏—Ü–∞ ùê¥. 
	
	Alu = lu(A)
	@btime lu(A)
	
end
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° dad468c8-d0da-4c92-80ba-073a9fbf1fc8
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
function is_productive_matrix(matrix, size)
        ans=""
         #–µ–¥–∏–Ω–∏—á–Ω–∞—è –º–∞—Ç—Ä–∏—Ü–∞
        E = [1 0; 0 1]
        # –∑–∞–¥–∞–¥–∏–º –ª—é–±—ã–µ –Ω–µ–æ—Ç—Ä–∏—Ü–∞—Ç–µ–ª—å–Ω—ã–µ —á–∏—Å–ª–∞
        Y = rand(0:1000, size)
        # –ü–æ —Ñ–æ—Ä–º—É–ª–µ –≤—ã—á–∏—Å–ª–∏–º x - A*x = y
        S = E - matrix
        # –Ω–∞–π–¥–µ–º –∑–Ω–∞—á–µ–Ω–∏—è x
        X = S\Y
        # —Ç–µ–ø–µ—Ä—å –ø—Ä–æ–≤–µ—Ä–∏–º –µ—Å—Ç—å –ª–∏ —Å—Ä–µ–¥–∏ x –æ—Ç—Ä—Ü–∏–∞—Ç–µ–ª—å–Ω–æ–µ —á–∏—Å–ª–æ
        for i in 1:1:size
            if X[i] < 0
                ans = "–ú–∞—Ç—Ä–∏—Ü–∞ –Ω–µ–ø—Ä–æ–¥—É–∫—Ç–∏–≤–Ω–∞—è"
                break
            else
                ans = "–ú–∞—Ç—Ä–∏—Ü–∞ –ø—Ä–æ–¥—É–∫—Ç–∏–≤–Ω–∞—è"
            end
        end
        return ans
    end
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° ea4f7981-b382-4724-b4b9-02ed4836e3ad
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
function is_productive_matrix_2(matrix, size)
        # –µ–¥–∏–Ω–∏—á–Ω–∞—è –º–∞—Ç—Ä–∏—Ü–∞
        ans = ""
        E = [1 0; 0 1]
        matrix_new = E - matrix
        inv_matrix_new = inv(matrix_new)
        for i in 1:1:size
            for j in 1:1:size
                if inv_matrix_new[i, j] < 0
                    ans = "–ú–∞—Ç—Ä–∏—Ü–∞ –Ω–µ–ø—Ä–æ–¥—É–∫—Ç–∏–≤–Ω–∞—è"
                    break
                else
                    ans = "–ú–∞—Ç—Ä–∏—Ü–∞ –ø—Ä–æ–¥—É–∫—Ç–∏–≤–Ω–∞—è"
                end
            end
        end
        return ans
    end
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° 361411bd-8fe5-4c80-894e-e80ee199e735
function is_productive_matrix_3(matrix, size)
        ans=""
        # –Ω–∞–π–¥–µ–º —Å–æ–±—Å—Ç–≤–µ–Ω–Ω—ã–µ –∑–Ω–∞—á–µ–Ω–∏—è –ø–µ—Ä–µ–¥–∞–Ω–Ω–æ–π –º–∞—Ç—Ä–∏—Ü—ã
        eigenvalues = eigvals(matrix)
        for i in 1:1:size
            if abs(eigenvalues[i]) > 1
                ans = "–ú–∞—Ç—Ä–∏—Ü–∞ –Ω–µ–ø—Ä–æ–¥—É–∫—Ç–∏–≤–Ω–∞—è"
                break
            else
                ans = "–ú–∞—Ç—Ä–∏—Ü–∞ –ø—Ä–æ–¥—É–∫—Ç–∏–≤–Ω–∞—è"
            end
        end
        return ans
    end

# ‚ïî‚ïê‚ï° 8766e547-a4c3-487d-8476-d8c41cc69e1c
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°

	begin
		matrix1 = [1 -2; -2 1]
		Asym = matrix1 + matrix1'
		AsymEig = eigen(Asym)
		inv(AsymEig.vectors) * matrix1 * AsymEig.vectors
	end
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° 1087cd86-d1ae-4ad8-aa1a-99ec3e372e3a
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
begin
		matrix = [1 -2; -2 3]
		Asym = matrix + matrix'
		AsymEig = eigen(Asym)
		inv(AsymEig.vectors) * matrix * AsymEig.vectors
	end
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° 7d2fc667-8a89-4d8d-b0d7-0f06058e5834
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
begin
	matrix1 = [1 2; 3 4]
	matrix2 = ([1 2; 3 4]) * (1/2)
	matrix3 = ([1 2; 3 4]) * (1/10)
	println(is_productive_matrix(matrix1, 2))
	println(is_productive_matrix(matrix2, 2))
	println(is_productive_matrix(matrix3, 2))
end
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° b78db58f-fedd-4f98-8aa4-0cc5e505fa86
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
begin
	matrix1 = [1 2; 3 1]
	matrix2 = ([1 2; 3 1]) * (1/2)
	matrix3 = ([1 2; 3 1]) * (1/10)
	
	println(is_productive_matrix_2(matrix1,2))
	println(is_productive_matrix_2(matrix2,2))
	println(is_productive_matrix_2(matrix3,2))
end
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° cc522c93-e7a2-4dca-8b3c-37de9255e7cf
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
begin
	A = [140 97 74 168 131
	97 106 89 131 36
	74 89 152 144 71
	168 131 144 54 142
	131 36 71 142 36]
	# eigenvalues = eigvals(A)
end
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° 53d03e60-6dba-41a4-8c44-20093a0d9e50
begin
	matrix1 = [1 2; 3 1]
	matrix2 = ([1 2; 3 1])*(1/2)
	matrix3 = ([1 2; 3 1])*(1/10)
	matrix4 = [0.1 0.2 0.3; 0 0.1 0.2; 0 0.1 0.3]
	println(is_productive_matrix_3(matrix1,2))
	println(is_productive_matrix_3(matrix2,2))
	println(is_productive_matrix_3(matrix3,2))
	println(is_productive_matrix_3(matrix4,2))
	
end

# ‚ïî‚ïê‚ï° dd46cf7f-9ec7-404f-8879-f70f2b6c2e48
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
begin
	# –¢—Ä—ë—Ö–¥–∏–∞–≥–æ–Ω–∞–ª—å–Ω–∞—è –º–∞—Ç—Ä–∏—Ü–∞ 1000000 —Ö 1000000:
	n = 1000000;
	A = SymTridiagonal(randn(n), randn(n-1))

end
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° b4d4e8b3-481e-46c8-af60-b907008a4343
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
begin
		matrix = [1 -2 0; -2 1 2; 0 2 0]
		Asym = matrix + matrix'
		AsymEig = eigen(Asym)
		inv(AsymEig.vectors) * matrix * AsymEig.vectors
	end
  ‚ï†‚ïê‚ï° =#

