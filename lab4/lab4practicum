# ‚ïî‚ïê‚ï° b58f8490-9110-11ee-2f15-7924757ab41a
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
begin
	# –ú–∞—Å—Å–∏–≤ 4x3 —Å–æ —Å–ª—É—á–∞–π–Ω—ã–º–∏ —Ü–µ–ª—ã–º–∏ —á–∏—Å–ª–∞–º–∏ (–æ—Ç 1 –¥–æ 20):
	a = rand(1:20,(4,3))
	# –ü–æ—ç–ª–µ–º–µ–Ω—Ç–Ω–∞—è —Å—É–º–º–∞:
	sum(a)
	# –ü–æ—ç–ª–µ–º–µ–Ω—Ç–Ω–∞—è —Å—É–º–º–∞ –ø–æ —Å—Ç–æ–ª–±—Ü–∞–º:
	sum(a,dims=1)
	# –ü–æ—ç–ª–µ–º–µ–Ω—Ç–Ω–∞—è —Å—É–º–º–∞ –ø–æ —Å—Ç—Ä–æ–∫–∞–º:
	sum(a,dims=2)
	# –ü–æ—ç–ª–µ–º–µ–Ω—Ç–Ω–æ–µ –ø—Ä–æ–∏–∑–≤–µ–¥–µ–Ω–∏–µ:
	prod(a)
	# –ü–æ—ç–ª–µ–º–µ–Ω—Ç–Ω–æ–µ –ø—Ä–æ–∏–∑–≤–µ–¥–µ–Ω–∏–µ –ø–æ —Å—Ç–æ–ª–±—Ü–∞–º:
	prod(a,dims=1)
	# –ü–æ—ç–ª–µ–º–µ–Ω—Ç–Ω–æ–µ –ø—Ä–æ–∏–∑–≤–µ–¥–µ–Ω–∏–µ –ø–æ —Å—Ç—Ä–æ–∫–∞–º:
	prod(a,dims=2)
end
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° 9731a6fb-6e16-4cfb-af6a-c90d4aab3ee3
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
begin
	# –í—ã—á–∏—Å–ª–µ–Ω–∏–µ —Å—Ä–µ–¥–Ω–µ–≥–æ –∑–Ω–∞—á–µ–Ω–∏—è –º–∞—Å—Å–∏–≤–∞:
	mean(a)
	# –°—Ä–µ–¥–Ω–µ–µ –ø–æ —Å—Ç–æ–ª–±—Ü–∞–º:
	mean(a,dims=1)
	# –°—Ä–µ–¥–Ω–µ–µ –ø–æ —Å—Ç—Ä–æ–∫–∞–º:
	mean(a,dims=2)
end
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° 690b96f4-4785-49d1-842c-5e4eb6fb4e33
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
begin
	# –ú–∞—Å—Å–∏–≤ 4x4 —Å–æ —Å–ª—É—á–∞–π–Ω—ã–º–∏ —Ü–µ–ª—ã–º–∏ —á–∏—Å–ª–∞–º–∏ (–æ—Ç 1 –¥–æ 20):
	b = rand(1:20,(4,4))
	# –¢—Ä–∞–Ω—Å–ø–æ–Ω–∏—Ä–æ–≤–∞–Ω–∏–µ:
	transpose(b)
	# –°–ª–µ–¥ –º–∞—Ç—Ä–∏—Ü—ã (—Å—É–º–º–∞ –¥–∏–∞–≥–æ–Ω–∞–ª—å–Ω—ã—Ö —ç–ª–µ–º–µ–Ω—Ç–æ–≤):
	tr(b)
	# –ò–∑–≤–ª–µ—á–µ–Ω–∏–µ –¥–∏–∞–≥–æ–Ω–∞–ª—å–Ω—ã—Ö —ç–ª–µ–º–µ–Ω—Ç–æ–≤ –∫–∞–∫ –º–∞—Å—Å–∏–≤:
	diag(b)
	# –†–∞–Ω–≥ –º–∞—Ç—Ä–∏—Ü—ã:
	rank(b)
	# –ò–Ω–≤–µ—Ä—Å–∏—è –º–∞—Ç—Ä–∏—Ü—ã (–æ–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ –æ–±—Ä–∞—Ç–Ω–æ–π –º–∞—Ç—Ä–∏—Ü—ã):
	inv(b)
	# –û–ø—Ä–µ–¥–µ–ª–∏—Ç–µ–ª—å –º–∞—Ç—Ä–∏—Ü—ã:
	det(b)
	# –ü—Å–µ–≤–¥–æ–±—Ä–∞—Ç–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è –¥–ª—è –ø—Ä—è–º–æ—É–≥–æ–ª—å–Ω—ã—Ö –º–∞—Ç—Ä–∏—Ü:
	pinv(a)
	
end
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° 250027be-fa5c-499f-9cad-fe4c5b90318a
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
begin
	# –°–æ–∑–¥–∞–Ω–∏–µ –≤–µ–∫—Ç–æ—Ä–∞ X:
	X = [2, 4, -5]
	# –í—ã—á–∏—Å–ª–µ–Ω–∏–µ –µ–≤–∫–ª–∏–¥–æ–≤–æ–π –Ω–æ—Ä–º—ã:
	println(norm(X))
	# –í—ã—á–∏—Å–ª–µ–Ω–∏–µ p-–Ω–æ—Ä–º—ã:
	p = 1
	norm(X,p)
end
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° 09cf0f92-4267-4a97-935a-3e8e24f8274e
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
begin
	# –†–∞—Å—Å—Ç–æ—è–Ω–∏–µ –º–µ–∂–¥—É –¥–≤—É–º—è –≤–µ–∫—Ç–æ—Ä–∞–º–∏ X –∏ Y:
	X = [2, 4, -5];
	Y = [1,-1,3];
	norm(X-Y)
end
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° 8fe01fa9-930e-4a36-81c2-2732111dae22
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
# –ü—Ä–æ–≤–µ—Ä–∫–∞ –ø–æ –±–∞–∑–æ–≤–æ–º—É –æ–ø—Ä–µ–¥–µ–ª–µ–Ω–∏—é:
sqrt(sum((X-Y).^2))

  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° 5ada817d-cb2b-4b08-9035-dbb1122fb3d4
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
# –£–≥–æ–ª –º–µ–∂–¥—É –¥–≤—É–º—è –≤–µ–∫—Ç–æ—Ä–∞–º–∏:
acos((transpose(X)*Y)/(norm(X)*norm(Y)))
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° 80b2a223-4039-40b8-a377-8854594a2291
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
begin
	# –°–æ–∑–¥–∞–Ω–∏–µ –º–∞—Ç—Ä–∏—Ü—ã:
	d = [5 -4 2 ; -1 2 3; -2 1 0]
	# –í—ã—á–∏—Å–ª–µ–Ω–∏–µ –ï–≤–∫–ª–∏–¥–æ–≤–æ–π –Ω–æ—Ä–º—ã:
	opnorm(d)
	# –í—ã—á–∏—Å–ª–µ–Ω–∏–µ p-–Ω–æ—Ä–º—ã:
	p=1
	opnorm(d,p)
	# –ü–æ–≤–æ—Ä–æ—Ç –Ω–∞ 180 –≥—Ä–∞–¥—É—Å–æ–≤:
	rot180(d)
	# –ü–µ—Ä–µ–≤–æ—Ä–∞—á–∏–≤–∞–Ω–∏–µ —Å—Ç—Ä–æ–∫:
	reverse(d,dims=1)
	# –ü–µ—Ä–µ–≤–æ—Ä–∞—á–∏–≤–∞–Ω–∏–µ —Å—Ç–æ–ª–±—Ü–æ–≤
	reverse(d,dims=2)
end
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° ff375113-e3e1-4ea0-a648-db1a1951f34a
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
begin
	# –ú–∞—Ç—Ä–∏—Ü–∞ 2x3 —Å–æ —Å–ª—É—á–∞–π–Ω—ã–º–∏ —Ü–µ–ª—ã–º–∏ –∑–Ω–∞—á–µ–Ω–∏—è–º–∏ –æ—Ç 1 –¥–æ 10:
	A = rand(1:10,(2,3))
	# –ú–∞—Ç—Ä–∏—Ü–∞ 3x4 —Å–æ —Å–ª—É—á–∞–π–Ω—ã–º–∏ —Ü–µ–ª—ã–º–∏ –∑–Ω–∞—á–µ–Ω–∏—è–º–∏ –æ—Ç 1 –¥–æ 10:
	B = rand(1:10,(3,4))
	# –ü—Ä–æ–∏–∑–≤–µ–¥–µ–Ω–∏–µ –º–∞—Ç—Ä–∏—Ü A –∏ B:
	A*B
	
end
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° 2d05369a-8386-467f-9aaa-8918eb4aa03f
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
begin
	# –ï–¥–∏–Ω–∏—á–Ω–∞—è –º–∞—Ç—Ä–∏—Ü–∞ 3x3:
	Matrix{Int}(I, 3, 3)
	# –°–∫–∞–ª—è—Ä–Ω–æ–µ –ø—Ä–æ–∏–∑–≤–µ–¥–µ–Ω–∏–µ –≤–µ–∫—Ç–æ—Ä–æ–≤ X –∏ Y:
	X = [2, 4, -5]
	Y = [1,-1,3]
	dot(X,Y)
	# —Ç–æ–∂–µ —Å–∫–∞–ª—è—Ä–Ω–æ–µ –ø—Ä–æ–∏–∑–≤–µ–¥–µ–Ω–∏–µ:
	X'Y
end
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° f93fdf98-5c9f-4215-9556-9f39c44cbde2
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
begin
	# –ó–∞–¥–∞—ë–º –∫–≤–∞–¥—Ä–∞—Ç–Ω—É—é –º–∞—Ç—Ä–∏—Ü—É 3x3 —Å–æ —Å–ª—É—á–∞–π–Ω—ã–º–∏ –∑–Ω–∞—á–µ–Ω–∏—è–º–∏:
	A = rand(3, 3)
	# –ó–∞–¥–∞—ë–º –µ–¥–∏–Ω–∏—á–Ω—ã–π –≤–µ–∫—Ç–æ—Ä:
	x = fill(1.0, 3)
	# –ó–∞–¥–∞—ë–º –≤–µ–∫—Ç–æ—Ä b:
	b = A*x
	# –†–µ—à–µ–Ω–∏–µ –∏—Å—Ö–æ–¥–Ω–æ–≥–æ —É—Ä–∞–≤–Ω–µ–Ω–∏—è –ø–æ–ª—É—á–∞–µ–º —Å –ø–æ–º–æ—â—å—é —Ñ—É–Ω–∫—Ü–∏–∏ \
	# (—É–±–µ–∂–¥–∞–µ–º—Å—è, —á—Ç–æ x - –µ–¥–∏–Ω–∏—á–Ω—ã–π –≤–µ–∫—Ç–æ—Ä):
	A\b
end
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° 0af0c710-7e4e-4927-83a5-ce4c969a1601
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
# LU-—Ñ–∞–∫—Ç–æ—Ä–∏–∑–∞—Ü–∏—è:
Alu = lu(A)

  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° 8483eaac-1d4c-4893-8ebf-895c303322d1
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
begin
	# –ú–∞—Ç—Ä–∏—Ü–∞ –ø–µ—Ä–µ—Å—Ç–∞–Ω–æ–≤–æ–∫:
	Alu.P
	# –í–µ–∫—Ç–æ—Ä –ø–µ—Ä–µ—Å—Ç–∞–Ω–æ–≤–æ–∫:
	Alu.p
	# –ú–∞—Ç—Ä–∏—Ü–∞ L:
	Alu.L
	# –ú–∞—Ç—Ä–∏—Ü–∞ U:
	Alu.U
	
end
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° 3f5f8733-680f-4c36-b747-6955af1cfd0f
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
begin
	# –†–µ—à–µ–Ω–∏–µ –°–õ–ê–£ —á–µ—Ä–µ–∑ –º–∞—Ç—Ä–∏—Ü—É A:
	A\b
	# –†–µ—à–µ–Ω–∏–µ –°–õ–ê–£ —á–µ—Ä–µ–∑ –æ–±—ä–µ–∫—Ç —Ñ–∞–∫—Ç–æ—Ä–∏–∑–∞—Ü–∏–∏:
	Alu\b
	
end
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° eac1ff55-86ad-40dc-978e-90b3984f3d78
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
begin
	# –ê–Ω–∞–ª–æ–≥–∏—á–Ω–æ –º–æ–∂–Ω–æ –Ω–∞–π—Ç–∏ –¥–µ—Ç–µ—Ä–º–∏–Ω–∞–Ω—Ç –º–∞—Ç—Ä–∏—Ü—ã:
	# –î–µ—Ç–µ—Ä–º–∏–Ω–∞–Ω—Ç –º–∞—Ç—Ä–∏—Ü—ã A:
	det(A)
	# –î–µ—Ç–µ—Ä–º–∏–Ω–∞–Ω—Ç –º–∞—Ç—Ä–∏—Ü—ã A —á–µ—Ä–µ–∑ –æ–±—ä–µ–∫—Ç —Ñ–∞–∫—Ç–æ—Ä–∏–∑–∞—Ü–∏–∏:
	det(Alu)
end
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° 0590574c-5481-49af-8e44-e8db30d09e4b
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
# QR-—Ñ–∞–∫—Ç–æ—Ä–∏–∑–∞—Ü–∏—è:
Aqr = qr(A)
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° 3c28655e-282d-4e0b-a979-ee1f0d0fcda5
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
begin
	# –ú–∞—Ç—Ä–∏—Ü–∞ Q:
	Aqr.Q
	# –ú–∞—Ç—Ä–∏—Ü–∞ R:
	Aqr.R
	# –ü—Ä–æ–≤–µ—Ä–∫–∞, —á—Ç–æ –º–∞—Ç—Ä–∏—Ü–∞ Q - –æ—Ä—Ç–æ–≥–æ–Ω–∞–ª—å–Ω–∞—è:
	Aqr.Q'*Aqr.Q
	
end
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° 6a54e226-5f39-456b-826b-9deb9886bab9
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
begin
	# –°–∏–º–º–µ—Ç—Ä–∏–∑–∞—Ü–∏—è –º–∞—Ç—Ä–∏—Ü—ã A:
	Asym = A + A'
	# –°–ø–µ–∫—Ç—Ä–∞–ª—å–Ω–æ–µ —Ä–∞–∑–ª–æ–∂–µ–Ω–∏–µ —Å–∏–º–º–µ—Ç—Ä–∏–∑–æ–≤–∞–Ω–Ω–æ–π –º–∞—Ç—Ä–∏—Ü—ã:
	AsymEig = eigen(Asym)
	# –°–æ–±—Å—Ç–≤–µ–Ω–Ω—ã–µ –∑–Ω–∞—á–µ–Ω–∏—è:
	AsymEig.values
	#–°–æ–±—Å—Ç–≤–µ–Ω–Ω—ã–µ –≤–µ–∫—Ç–æ—Ä—ã:
	AsymEig.vectors
	# –ü—Ä–æ–≤–µ—Ä—è–µ–º, —á—Ç–æ –ø–æ–ª—É—á–∏—Ç—Å—è –µ–¥–∏–Ω–∏—á–Ω–∞—è –º–∞—Ç—Ä–∏—Ü–∞:
	inv(AsymEig)*Asym
end
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° 6fef692e-1a30-4fb7-9ae4-817bd029b67b
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
begin
	# –ú–∞—Ç—Ä–∏—Ü–∞ —Å —Ä–∞—Ü–∏–æ–Ω–∞–ª—å–Ω—ã–º–∏ —ç–ª–µ–º–µ–Ω—Ç–∞–º–∏:
	Arational = Matrix{Rational{BigInt}}(rand(1:10, 3, 3))/10
	# –ï–¥–∏–Ω–∏—á–Ω—ã–π –≤–µ–∫—Ç–æ—Ä:
	x = fill(1, 3)
	# –ó–∞–¥–∞—ë–º –≤–µ–∫—Ç–æ—Ä b:
	b = Arational*x
	# –†–µ—à–µ–Ω–∏–µ –∏—Å—Ö–æ–¥–Ω–æ–≥–æ —É—Ä–∞–≤–Ω–µ–Ω–∏—è –ø–æ–ª—É—á–∞–µ–º —Å –ø–æ–º–æ—â—å—é —Ñ—É–Ω–∫—Ü–∏–∏ \
	# (—É–±–µ–∂–¥–∞–µ–º—Å—è, —á—Ç–æ x - –µ–¥–∏–Ω–∏—á–Ω—ã–π –≤–µ–∫—Ç–æ—Ä):
	Arational\b
	# LU-—Ä–∞–∑–ª–æ–∂–µ–Ω–∏–µ:
	lu(Arational)
end
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° f04d387f-501a-49cc-8184-6a3757a215e0
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
begin
	v = [1,2,3,4,5]
	dot_v = dot(v,v)
end
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° bd003d6b-b06b-4817-aa13-f230454f8212
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
outer_v = v * v'
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° cb8ebdcc-2a2c-4b03-8086-26534ac34aab
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
begin
	K = [1 1; 1 -1]
	answers = [2; 3]
	K\answers
end
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° da06d5a7-5b59-406b-9579-4f22d575ed9a
# ‚ï†‚ïê‚ï° disabled = true
#=‚ï†‚ïê‚ï°
begin
	K2 = [1 1; 2 2]
	answers2 = [2; 4]
	K2\answers2
end
# —Ä–µ—à–µ–Ω–∏–π –±–µ—Å–∫–æ–Ω–µ—á–Ω–æ
  ‚ï†‚ïê‚ï° =#

# ‚ïî‚ïê‚ï° 8ac254f5-e427-4ca8-ab09-8c13031208a9
